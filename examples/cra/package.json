{
    "name": "berry-free-material-react-cra",
    "version": "3.3.1",
    "private": true,
    "homepage": "https://example.com/cra",
    "dependencies": {
        "@emotion/cache": "^11.9.3",
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/lab": "^5.0.0-alpha.88",
        "@mui/material": "^5.8.6",
        "@mui/system": "^5.8.6",
        "@mui/utils": "^5.8.6",
        "@tabler/icons": "^1.72.0",
        "apexcharts": "^3.35.3",
        "formik": "^2.2.9",
        "framer-motion": "^6.3.16",
        "material-ui-popup-state": "^4.0.1",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-apexcharts": "^1.4.0",
        "react-device-detect": "^2.2.2",
        "react-dom": "^18.2.0",
        "react-perfect-scrollbar": "^1.5.8",
        "react-redux": "^8.0.2",
        "react-router": "6.3.0",
        "react-router-dom": "6.3.0",
        "react-scripts": "^5.0.1",
        "redux": "^4.2.0",
        "safetest": "file:../..",
        "yup": "^0.32.11"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "(rm -rf ../../build/cra || true) && react-scripts build && mv build ../../build/cra",
        "test": "react-scripts test",
        "safetest": "react-scripts --inspect test --runInBand --url=http://localhost:3000/cra --testMatch '**/*.safetest.{j,t}s{,x}' --setupFilesAfterEnv ./setup-safetest.js",
        "safetest:ci": "npm run safetest -- --watchAll=false --bail=5 --ci=1 --docker=1 --url=https://safetest-two.vercel.app/cra --json --outputFile=../../build/cra/results.json",
        "safetest:regenerate-screenshots": "npm run safetest -- --watchAll=false --docker=1 --update-snapshot",
        "process:ci": "node -e 'require(\"safetest/process-action\")' -- --results=../../build/cra/results.json --artifacts=../../build/cra/artifacts --url=https://safetest-two.vercel.app/cra --build-url=. --bootstrapped-at=src",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    },
    "browserslist": {
        "production": [
            "defaults",
            "not IE 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.18.6",
        "@babel/eslint-parser": "^7.18.2",
        "@babel/preset-react": "^7.18.6",
        "eslint": "^8.19.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "7.0.1",
        "eslint-import-resolver-typescript": "3.2.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.0",
        "eslint-plugin-markdown": "^2.2.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "prettier": "^2.7.1",
        "sass": "^1.53.0"
    }
}
